<div class="row">
  <div class="col-sm-10 col-md-10"> 
    <h3>
      <a class="plan" href="#" data-id="<%= id %>"><%= marketing_name %></a>
    </h3>

    <div class="row">
      <div class="col-sm-5 col-md-5">
        <div class="issuer-name"> 
          <%= issuer_name %> 
        </div>
        
        <span class="metal-level <%= metal_level_name %>"><%= metal_level_name %></span>

      </div>

      <div class="col-md-7 col-sm-7">
          <span class="benefit-key">Deductible</span>
          <span class="benefit-value"><%= medical_deductible %></span> 
        <br>
          <span class="benefit-key">Out-of-Pocket Max</span>
          <span class="benefit-value"><%= medical_max_oop %></span>      
        <br>
          <span class="benefit-key">Primary Doctory Visit</span>
          <span class="benefit-value"><%= pcp_copay %></span> 
        <br>
      </div>
    </div>
  </div>
  <div class="col-sm-2 col-md-2">
    <% if ( gross_premium ) { %>
      <div class="aca-pricing-summary">
        <span class="net-premium-text"> $<%= Utils.Number.formatFixed(net_premium, 2) %> </span> 
        <span class="premium-frequency-text"> per month estimate</span>
        <% if (subsidy && subsidy > 0) { %>
        <span style="color: #777">was $<%= Utils.Number.formatFixed(gross_premium, 2) %> </span>
        <% } %>
      </div>


    <% } %>
  </div>
</div>
